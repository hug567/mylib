#!/usr/bin/expect -f

puts "for: ------------------------------------"
for {set i 0} {$i < 10} {incr i} {
    puts "i = $i"
}

puts "foreach: --------------------------------"
foreach  i { 1 3 5 7 9 } {
    puts "i = $i"
}

puts "while: ----------------------------------"
set i 0
while {$i < 10} {
    puts "i = $i"
    incr i
}
