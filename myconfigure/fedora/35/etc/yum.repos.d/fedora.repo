# Fedora 35 - 2022.05.01
[fedora]
name=Fedora $releasever - $basearch
baseurl=https://mirrors.aliyun.com/fedora/releases/$releasever/Everything/$basearch/os/
#baseurl=https://repo.huaweicloud.com/fedora/releases/$releasever/Everything/$basearch/os/
#baseurl=https://mirrors.163.com/fedora/releases/$releasever/Everything/$basearch/os/
#baseurl=https://mirrors.cloud.tencent.com/fedora/releases/$releasever/Everything/$basearch/os/
#metalink=https://mirrors.fedoraproject.org/metalink?repo=fedora-$releasever&arch=$basearch
enabled=1
countme=1
metadata_expire=7d
repo_gpgcheck=0
type=rpm
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-fedora-$releasever-$basearch
skip_if_unavailable=False

[fedora-debuginfo]
name=Fedora $releasever - $basearch - Debug
